    "test": "jest"
  },
  "dependencies": {
    "@test/shared": "^1.0.3",
    "react": "16.8.6",
    "react-native": "0.60.5",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.2",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "@types/react": "^16.9.2",
    "@types/react-native": "^0.60.14",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
